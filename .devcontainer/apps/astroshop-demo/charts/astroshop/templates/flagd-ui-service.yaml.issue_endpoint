apiVersion: v1
kind: Service
metadata:
  name: flagd-ui
  namespace: {{ .Release.Namespace }}
  labels:
    app.kubernetes.io/name: flagd-ui
    app.kubernetes.io/component: flagd
    app.kubernetes.io/part-of: opentelemetry-demo
spec:
  type: ClusterIP
  ports:
    - name: ui
      port: 4000
      targetPort: 4000
      protocol: TCP
  selector:
    app.kubernetes.io/name: flagd
    app.kubernetes.io/component: flagd
  